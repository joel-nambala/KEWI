
@{
    ViewBag.Title = "Fees PDF";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="mt-2 mb-3">
    <div class="d-flex justify-content-between align-items-center">
        <h3>Fees Statement</h3>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("index","dashboard")">Home</a></li>
                <li class="breadcrumb-item active" aria-current="page">@ViewBag.Title</li>
            </ol>
        </nav>
    </div>
</div>

<div class="card">
    <div class="card-header">
        <h3 class="card-title small">Individual Fees Statement</h3>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                @if (TempData["Error"] != null)
                {
                    <div class="alert alert-danger">@TempData["Error"]</div>
                }
            </div>
            <div class="col-md-12">
                <embed src="@ViewBag.filepath" type="application/pdf" width="100%" height="500" />
            </div>
        </div>
    </div>
</div>